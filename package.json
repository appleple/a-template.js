{
  "name": "a-template",
  "version": "0.3.1",
  "author": "steelydylan",
  "description": "Simple Template Engine",
  "license": "MIT",
  "scripts": {
    "bundle": "browserify ./src/index.js -t babelify -p licensify --standalone aTemplate --outfile ./build/a-template.js",
    "minifyjs": "uglifyjs ./build/a-template.js --comments 'license' -o ./build/a-template.min.js",
    "build": "npm run bundle && npm run minifyjs",
    "build:lib": "babel src --out-dir lib",
    "watch": "watch 'npm run build && npm run build:lib' ./src/",
    "patch": "npm version patch && node ./tools/index.js",
    "minor": "npm version minor &&  node ./tools/index.js",
    "major": "npm version major && node ./tools/index.js",
    "publish": "np"
  },
  "main": "./lib/index.js",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "co": "^4.6.0",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "fs-extra": "^2.1.2",
    "licensify": "^3.1.2",
    "node-cmd": "^2.0.0",
    "uglifyjs": "^2.4.10",
    "watch": "^1.0.1",
    "watchify": "^3.7.0"
  },
  "dependencies": {
    "array.prototype.find": "^2.0.4",
    "delegate": "^3.1.2",
    "morphdom": "^2.3.1",
    "np": "^2.16.0"
  }
}
