{
  "name": "a-template",
  "version": "0.2.3",
  "author": "steelydylan",
  "description": "Simple Template Engine",
  "license": "MIT",
  "scripts": {
    "bundle": "browserify ./src/index.js -t [ babelify --presets [ es2015 ] ] -p licensify --standalone aTemplate --outfile ./build/a-template.js",
    "minifyjs": "uglifyjs ./build/a-template.js --comments 'license' -o ./build/a-template.min.js",
    "build": "npm run bundle && npm run minifyjs",
    "build:lib": "babel ./src/index.js --presets es2015 --out-file ./index.js",
    "watch": "watch 'npm run build && npm run build:lib' ./src/",
    "patch": "npm version patch && node ./tools/index.js",
    "minor": "npm version minor &&  node ./tools/index.js",
    "major": "npm version major && node ./tools/index.js"
  },
  "main": "./index.js",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "licensify": "^3.1.2",
    "uglifyjs": "^2.4.10",
    "watch": "^1.0.1",
    "watchify": "^3.7.0",
    "node-cmd": "^2.0.0",
    "co": "^4.6.0",
    "fs-extra": "^2.1.2"
  },
  "dependencies": {
    "delegate": "^3.1.2",
    "morphdom": "^2.3.1"
  }
}
